node{
tool name: 'Terraform', type: 'terraform'
stage('gitcheckout'){
git branch: 'main', credentialsId: 'github', url: 'https://github.com/Jyothiganaparthi/terraform4.git'
}
stage('init'){
sh 'terraform init -reconfigure'
}
stage('plan'){
sh 'terraform plan --var-file terraform.tfvars -lock=false'
}
stage('apply){
sh 'terraform apply --var-file terraform.tfvars --auto-approve -lock=false'
}
}
